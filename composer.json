{
    "name": "gotoroho/dbal-clickhouse",
    "type": "library",
    "description": "Doctrine DBAL driver for ClickHouse",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.1",
        "doctrine/dbal": "^3.2",
        "smi2/phpclickhouse": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^0.12"
    },
    "autoload": {
        "psr-4": {
            "DBALClickHouse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DBALClickHouse\\Tests\\": "tests/"
        }
    }
}
